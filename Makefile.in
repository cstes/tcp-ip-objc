
OBJC=objc
MFLAGS=@MFLAGS@
LIBS=-lsocket  -lnsl

.SUFFIXES: .m 

.m.o:
	$(OBJC) $(MFLAGS) -c $*.m

default : daytime daytime-dgram nbdaytime daytime-select daytime-server byteorder

daytime : daytime.o
	$(OBJC) -o daytime $? $(LIBS)

daytime-dgram : daytime-dgram.o
	$(OBJC) -o daytime-dgram $? $(LIBS)

daytime-server : daytime-server.o
	$(OBJC) -o daytime-server $? $(LIBS)

daytime-select : daytime-select.o
	$(OBJC) -o daytime-select $? $(LIBS)

byteorder : byteorder.o
	$(OBJC) -o byteorder $? $(LIBS)

nbdaytime : nbdaytime.o
	$(OBJC) -o nbdaytime $? $(LIBS)

clean::
	rm -f daytime daytime-dgram daytime-server nbdaytime daytime-select byteorder
	rm -rf *.o

distclean : clean
	rm -rf Makefile config.h config.status config.log autom4te.cache

