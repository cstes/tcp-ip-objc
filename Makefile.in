
OBJC=objc
MFLAGS=@MFLAGS@
LIBS=-lsocket  -lnsl

.SUFFIXES: .m 

.m.o:
	$(OBJC) -c $*.m $(MFLAGS)

default : daytime daytime-server byteorder

daytime : daytime.o
	$(OBJC) -o daytime $? $(LIBS)

daytime-server : daytime-server.o
	$(OBJC) -o daytime-server $? $(LIBS)

byteorder : byteorder.o
	$(OBJC) -o byteorder $? $(LIBS)

clean::
	rm -f daytime daytime-server byteorder
	rm -rf *.o

distclean : clean
	rm -rf Makefile config.h config.status config.log autom4te.cache

